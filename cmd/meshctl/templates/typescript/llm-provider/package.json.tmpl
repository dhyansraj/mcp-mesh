{
  "name": "{{ .Name }}",
  "version": "1.0.0",
  "description": "{{ if .Description }}{{ .Description }}{{ else }}MCP Mesh LLM provider for {{ .Model }}{{ end }}",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "tsc",
    "dev": "tsx watch src/index.ts"
  },
  "dependencies": {
    "@mcpmesh/sdk": "^0.8.0",
{{ if contains .Model "anthropic" }}
    "@ai-sdk/anthropic": "^1.0.0",
{{ else if contains .Model "openai" }}
    "@ai-sdk/openai": "^1.0.0",
{{ else if contains .Model "google" }}
    "@ai-sdk/google": "^1.0.0",
{{ else }}
    "@ai-sdk/openai": "^1.0.0",
{{ end }}
    "ai": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0"
  }
}
